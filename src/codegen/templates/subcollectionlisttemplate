    def list(self, **kwargs):
%(docs)s
        url = '%(url)s'

        result = self.__getProxy().get(
            url=UrlHelper.replace(
                url,
                %(url_params)s
            )
        ).get_%(actual_resource_name_lc)s()

        return ParseHelper.toSubCollection(
            %(encapsulating_resource)s,
            self.parentclass,
            FilterHelper.filter(
                result,
                kwargs
            ),
            context=self.context
        )

